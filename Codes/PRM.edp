  k1 = 0.01;
  b = 0.01;
  c = 0.1;
  r = 0.01;
  k2 = 0.1;
  s = 1;

  //Rescaled prms
  epsi2 = D1/(Lx^2*(c+r));
  D = D2/(Lx^2*k1);
  tau = (c+r)/k1;
  beta = r/k1;
  gamma = (c+r)*k1^2/(k2*b^2);

  //prm for auxin
  nu = 1.5;

  //stationary point or initialization
  u0 = b/r;
  v0 = (b*r*(c+r)) / (k1*r^2 + k2*b^2);

  //F coefficients
  tildeD1 = D1;
  tildeD2 = D2;
  tildea1 = -(c+r);
