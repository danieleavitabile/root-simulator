if(prm == "Set 1")
{
  //Table1, set1 from ref.[10]
  Nx = 60;
  Ny = 60;

  Lx = 50; Ly = 20;
  D1 = 0.1; D2 = 10;
  k1 = 0.01;
  b = 0.01;
  c = 0.1;
  r = 0.01;
  k2 = 0.1;
  s = (Lx/Ly)^2;

  //Rescaled prms
  epsi2 = D1/(Lx^2*(c+r));
  D = D2/(Lx^2*k1);
  tau = (c+r)/k1;
  beta = r/k1;
  gamma = (c+r)*k1^2/(k2*b^2);

  //prm for auxin
  nu = 1.5;

  //stationary point or initialization
  u0 = 1./(gamma*beta);
  v0 = tau*beta*gamma/(tau + beta^2*gamma);
  // real uu0 =1./(gamma*beta);
  // real vv0 = tau*beta*gamma/(tau + beta^2*gamma);
  // func u0 = uu0* exp(-10*x);
  // func v0 = vv0 * exp(-10*x);

  //F coefficients:
  tildeD1 = epsi2;
  tildeD2 = D / tau;
  tildea1 = - 1;
  tildea2 = - 1/ tau;
  tildeb1 = 1/(gamma*tau);
  tildeb2 = gamma*(1 - beta/tau);

  f2 = 1/tau;
}
else if(prm == "V1 matlab")
{
  Nx = 100; Ny = 50;

  Lx = 2; Ly = 1;
